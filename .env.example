# API Configuration
ENVSAFE_API_URL=https://www.envsafe.dev
ENVSAFE_DASHBOARD_URL=https://www.envsafe.dev
ENVSAFE_TOKEN=your-token-here

# Project Context (Integration Tests)
ENVSAFE_WORKSPACE_ID=your-workspace-id
ENVSAFE_PROJECT_ID=your-project-id
ENVSAFE_PROJECT_NAME=test-project
ENVSAFE_ENV=development

# Rotation Settings
ENVSAFE_ROTATION_ENABLED=false
ENVSAFE_ROTATION_INTERVAL=30
ENVSAFE_ROTATION_EXCLUDE=NODE_ENV,PORT

# Docker Configuration
ENVSAFE_WATCH=true
ENVSAFE_SHMEM_SIZE=1048576

# Application Environment (Example)
NODE_ENV=development
PORT=3000
DEBUG=true
API_KEY=test-key-12345
JWT_SECRET=test-secret-67890
